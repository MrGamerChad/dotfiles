<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://openbox.org/                 file:///usr/share/openbox/menu.xsd">
	<menu id="root-menu" label="Openbox 3.5">
		<item label="Minecraft">
			<action name="Execute">
				<execute>java -jar ~/Downloads/Minecraft.jar</execute>
			</action>
		</item>
		<menu execute="/usr/bin/openbox-menu lxde-applications.menu" id="openbox-menu" label="Applications"/>
		<item label="Documents">
			<action name="Execute">
				<execute>thunar ~/Documents</execute>
			</action>
		</item>
		<menu id="Preferences" label="Preferences">
			<menu id="root-menu-525118" label="Openbox Config">
				<item label="Edit autostart">
					<action name="Execute">
						<execute>
							mousepad ~/.config/openbox/autostart
						</execute>
					</action>
				</item>
				<item label="GUI Menu Editor">
					<action name="Execute">
						<execute>
							obmenu
						</execute>
					</action>
				</item>
				<item label="GUI Config Tool">
					<action name="Execute">
						<execute>
							obconf
						</execute>
					</action>
				</item>
				<item label="Key Editor">
					<action name="Execute">
						<execute>
							obkey
						</execute>
					</action>
				</item>
				<item label="Reconfigure">
					<action name="Reconfigure"/>
				</item>
				<item label="Restart">
					<action name="Restart"/>
				</item>
			</menu>
			<menu id="root-menu-23433" label="Take Screenshot">
				<item label="Now">
					<action name="Execute">
						<execute>scrot -d 0.8 '%Y-%m-%d--%s_$wx$h_scrot.png' -e 'mv $f ~/ &amp; feh ~/$f'</execute>
					</action>
				</item>
				<item label="In 5 Seconds...">
					<action name="Execute">
						<execute>
							scrot -d 5 '%Y-%m-%d--%s_$wx$h_scrot.png' -e 'mv $f ~/ &amp; gpicview ~/$f'
						</execute>
					</action>
				</item>
				<item label="In 10 Seconds...">
					<action name="Execute">
						<execute>
							scrot -d 10 '%Y-%m-%d--%s_$wx$h_scrot.png' -e 'mv $f ~/ &amp; gpicview ~/$f'
						</execute>
					</action>
				</item>
				<item label="Selected Area... (click &amp; drag mouse)">
					<action name="Execute">
						<execute>
							scrot -s '%Y-%m-%d--%s_$wx$h_scrot.png' -e 'mv $f ~/ &amp; gpicview ~/$f'
						</execute>
					</action>
				</item>
				<item label="Record Desktop">
					<action name="Execute">
						<execute>terminator -e &quot;ffmpeg -video_size 1920x1080 -framerate 25 -f x11grab -i :0.0+100,200 output.mp4&quot;</execute>
					</action>
				</item>
			</menu>
			<menu id="root-menu-571948" label="tint2 config">
				<item label="Edit tint2rc">
					<action name="Execute">
						<execute>
							mousepad ~/.config/tint2/tint2rc
						</execute>
					</action>
				</item>
				<item label="Tint Wizard">
					<action name="Execute">
						<execute>
							tintwizard.py
						</execute>
					</action>
				</item>
			</menu>
			<item label="Input Device Preferences">
				<action name="Execute">
					<execute>
						lxinput
					</execute>
				</action>
			</item>
			<item label="Screen Resolution">
				<action name="Execute">
					<execute>
						arandr
					</execute>
				</action>
			</item>
			<item label="User Interface Settings">
				<action name="Execute">
					<execute>
						lxappearance
					</execute>
				</action>
			</item>
			<item label="Wallpaper">
				<action name="Execute">
					<execute>
						nitrogen
					</execute>
				</action>
			</item>
		</menu>
		<menu id="root-menu-36119" label="Windows">
			<item label="Fireworks">
				<action name="Execute">
					<execute>playonlinux --run &quot;Fireworks&quot;</execute>
				</action>
			</item>
			<item label="Illustrator">
				<action name="Execute">
					<execute>playonlinux --run &quot;Illustrator&quot;</execute>
				</action>
			</item>
			<item label="Photoshop">
				<action name="Execute">
					<execute>playonlinux --run &quot;Photoshop&quot;</execute>
				</action>
			</item>
			<separator/>
			<item label="Virtualbox">
				<action name="Execute">
					<execute>urxvt -e sh -c &quot;sudo modprobe vboxdrv &amp;&amp; virtualbox&quot;</execute>
				</action>
			</item>
		</menu>
		<separator/>
		<item label="Exit">
			<action name="Execute">
				<execute>
                        oblogout
                        </execute>
			</action>
		</item>
	</menu>
</openbox_menu>
